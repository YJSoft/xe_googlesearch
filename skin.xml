<?xml version="1.0" encoding="UTF-8"?>
<skin version="0.2">
    <title xml:lang="ko">통합검색 구글 검색 스킨</title>
    <title xml:lang="en">The Google Skin of the Integrated Search Module</title>
    <description xml:lang="ko">통합검색 구글 검색 스킨</description>
    <description xml:lang="en">The Google Skin of the Integrated Search Module</description>
    <version>0.1</version>
    <date>2013-10-03</date>

    <author email_address="yjsoft@yjsoft.pe.kr" link="http://yjsoft.pe.kr/">
        <name xml:lang="ko">YJSoft</name>
        <name xml:lang="vi">YJSoft</name>
        <name xml:lang="zh-CN">YJSoft</name>
        <name xml:lang="jp">YJSoft</name>
        <name xml:lang="en">YJSoft</name>
        <name xml:lang="zh-TW">YJSoft</name>
        <name xml:lang="tr">YJSoft</name>
    </author>

    <colorset>
        <color name="normal">
            <title xml:lang="ko">기본</title>
            <title xml:lang="en">Default</title>
        </color>
        <color name="simple">
            <title xml:lang="ko">Simplestrap용</title>
            <title xml:lang="en">For Simplestrap</title>
        </color>
    </colorset>

    <extra_vars>
        <var name="memo" type="textarea">
            <title xml:lang="ko">설명</title>
            <title xml:lang="zh-CN">说明</title>
            <title xml:lang="jp">説明</title>
            <title xml:lang="en">Description</title>
            <title xml:lang="vi">Mô tả</title>
            <title xml:lang="zh-TW">說明</title>
            <title xml:lang="tr">Açıklama</title>
            <description xml:lang="ko">검색결과 상단에 결과가 출력됩니다.</description>
            <description xml:lang="zh-CN">显示在搜索结果上端。</description>
            <description xml:lang="jp">検索結果が上段に表示されます。</description>
            <description xml:lang="en">Result will be displayed on top of search result.</description>
            <description xml:lang="vi">Mô tả sẽ hiển thị trên đầu trang kết quả tìm kiếm.</description>
            <description xml:lang="zh-TW">在最頂端顯示搜尋結果。</description>
            <description xml:lang="tr">Sonuç, arama sonuçlarının en üstünde gösterilecektir</description>
        </var>
		<var name="googleSearchCX" type="textarea">
            <title xml:lang="ko">구글검색 CX</title>
            <description xml:lang="ko">CX 코드를 입력해 주세요.</description>
        </var>
    </extra_vars>
</skin>
